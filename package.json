{
  "name": "node-sequelize",
  "version": "1.0.0",
  "description": "Node.js Rest Apis with Express, Sequelize & MySQL",
  "main": "server.js",
  "scripts": {
    "start": "nodemon server.js",
    "migrate:test": "NODE_ENV=test sequelize db:migrate --config config/db.json",
    "migrate:dev": "NODE_ENV=development sequelize db:migrate --config config/db.json",
    "migrate:prod": "NODE_ENV=production sequelize db:migrate --config config/db.json",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "nodejs",
    "express",
    "rest",
    "api",
    "sequelize",
    "mysql"
  ],
  "author": "bezkoder",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "mysql2": "^2.0.2",
    "nodemon": "^2.0.7",
    "sequelize": "^5.21.2"
  },
  "devDependencies": {
    "sequelize-cli": "^6.2.0"
  }
}
